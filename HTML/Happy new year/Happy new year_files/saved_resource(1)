<script>
    // Function to show the modal
    function showModal() {
        const modal = document.getElementById("myModal");
        modal.style.display = "flex";
    }

    // Function to close the modal and set a session-based flag
    function closeModal() {
        const modal = document.getElementById("myModal");
        modal.style.display = "none";

        // Set a session-based flag to prevent modal reappearance in the same session
    }

    // Check if the modal was already shown in the current session

    // Check if the user has scrolled 25% of the window height

    // Add click event listeners to open and close the modal
    const openBtnD = document.getElementById("openModalDesktop");
    const openBtnM = document.getElementById("openModalMobile");
    const closeBtn = document.getElementById("closeModal");

    openBtnD.addEventListener("click", showModal);
    openBtnM.addEventListener("click", showModal);
    closeBtn.addEventListener("click", closeModal);
</script>
