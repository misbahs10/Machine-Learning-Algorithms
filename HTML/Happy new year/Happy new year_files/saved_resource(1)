        var pagination_next_page_number = '',
            pagination_available_pages_number = '';
        var serachContentApi = "https://blog.gratefulness.me/ghost/api/v4/content/posts/?key=" + window.contentApiKey + "&limit=all&fields=id,title,excerpt,primary_tag,feature_image,custom_excerpt,url,published_at&formats=plaintext&include=tags";
