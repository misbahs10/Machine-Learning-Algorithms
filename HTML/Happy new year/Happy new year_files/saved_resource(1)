<script>
$(document).ready(function () {
    // Mobile Menu Trigger
    $('.gh-burger').click(function () {
        $('body').toggleClass('gh-head-open');
    });
    // FitVids - Makes video embeds responsive
    $(".gh-content").fitVids();

    // bar-icon        
    $('.bar-icon').on('click', function(){
        $(this).toggleClass('active-bar');
        $('#nav').toggleClass('active')
        $('body').toggleClass('overflow-hidden')
    }); 

    // Load more posts JS
    var nextPage = $("link[rel=next]").attr("href"),
        a = $(".load-more-posts"),
        b = $(".loadmore-section");

        if (!nextPage) {
        b.remove();
        }
        a.click(function (n) {
            n.preventDefault();
            var i = nextPage.split(/page/)[0] + "page/" + pagination_next_page_number + "/";
            $.ajax({
                url: i,
                beforeSend: function () {
                    a.addClass('loading')
                },
            }).done(function (data) {
                var posts = $(".js-post-card", data),
                    postwrapper = $(".js-post-wrapper");
                postwrapper.append(posts);
                a.removeClass('loading');
                pagination_next_page_number++;
                pagination_next_page_number > pagination_available_pages_number && b.remove()
            });
        });

    // gh-cus-button
    $('.gh-cus-button').click(function () {
        $('.member-subscribe').addClass('active');
    });

    // search-bar
    $('.search-bar').click (function (e) {
        e.preventDefault()
        $(this).toggleClass('search-active')
        $('.search-area').toggleClass('active').slideToggle();
    });

    // Search results
    var posts = [],
        searchKey = $('#search-input'),
        searchContent = $('#search-full-content'),
        searchResultnum = $('.no-result'),
        overlay = $('.overlay-content')
    // search-bar
    $('.search-bar').on('click', function (e) {
        e.preventDefault()
        overlay.addClass('active').on('click', function (e) {
            e.stopPropagation()
        })

            e.stopPropagation()
            $('search-input-area')
            .delay(4000)
            .fadeIn()
            $('body').addClass('body-overflow')
            searchKey.focus()


        if (posts.length == 0 && typeof serachContentApi !== undefined) {
            $.get(serachContentApi)
                .done(function(data) {
                    posts = data.posts
                    search()
                })
                .fail(function(err) {})
        }
    });
    //  click on close icon input text clear
    function seacrhEvent() {
        overlay.removeClass('active')
        $('body').removeClass('body-overflow')
        searchKey.val('')
        searchContent.empty()
        searchResultnum.removeClass('d-block')
    }
    $('.bar-dismiss').on('click', function () {
        seacrhEvent()
    });
    $(document).on('click', function () {
        seacrhEvent()
    });
    $(document).keydown(function (event) {
        if (event.keyCode == 27) {
            seacrhEvent()
        }
    })

    function search(){
        var options = {
            shouldSort: true,
            tokenize: true,
            matchAllTokens: true,
            threshold: 0,
            location: 0,
            distance: 100,
            maxPatternLength: 32,
            minMatchCharLength: 1,
            keys: [
                {
                    name: "title",
                    weight: 0.3,
                }
            ]
        };
        var fuse = new Fuse(posts, options)
          
        searchKey.keyup(function(){
            var value = this.value,
            search = fuse.search(value),
            output = '',
            language = $('html').attr('lang')

            searchResultnum.addClass('d-block')
            searchResultnum.children('span').html(search.length)
            if(posts.length > 0){
            $.each(search, function(key, val) {
                    var pubDate = new Date(
                        val.published_at
                    ).toLocaleDateString(language, {
                        day: 'numeric',
                        month: 'long',
                        year: 'numeric',
                    })
                    output += `
                    <a class="single-search-result" href="${val.url}">
                        <div class="single-result-item">
                            ${(val.feature_image)?`
                                <div class="search-feature-image">
                                    <img src="${val.feature_image}" alt="">
                                </div>
                            `:''}
                            <h5 class="search-post-title">${val.title}</h5>
                        </div>
                    </a>
                    `
                })

                searchContent.html(output)
            }
        })
    }

    
    // header height
    function bodyTopPadding() {
    var headerHeight = $('.header-wrapper').outerHeight()
        $('body').css('paddingTop', headerHeight + 'px')
        }
    bodyTopPadding()

    $(window).resize(function () {
        bodyTopPadding()
    })

    $(window).scroll(function () {
    var scroll = $(window).scrollTop()
       if (scroll > 0) {
           $('.header-wrapper').removeClass('fixed')
        } else {
            $('.').addClass('fixed')
        }
    })

    // Header navigation sticky on scroll
    const headerWrapper = document.querySelector('.header-wrapper');
    var lastScrollY = window.scrollY;

    window.addEventListener('scroll', (event) => {
        var currScroll = window.pageYOffset;
        if (lastScrollY < window.scrollY) {
            headerWrapper.classList.add('header-wrapper--scrolled');
            headerWrapper.classList.remove('header-wrapper--scrolled-down');
        } else {
            headerWrapper.classList.remove('header-wrapper--scrolled');
            headerWrapper.classList.add('header-wrapper--scrolled-down');
        }
        lastScrollY = window.scrollY;

        if(currScroll < 200){
            headerWrapper.classList.remove('header-wrapper--scrolled');
            headerWrapper.classList.remove('header-wrapper--scrolled-down');
        }
    });

    // const progressBar = document.querySelector('.progress-bar');
    // const bottomPopup = document.querySelector('.popup-section');
    // const bottomPopupMaybe = document.querySelector('.js-later');

    // if (progressBar) {
